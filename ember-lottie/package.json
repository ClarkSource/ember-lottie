{
  "name": "ember-lottie",
  "version": "0.0.1",
  "description": "Render vector animations with lottie-web in Ember.js",
  "keywords": [
    "ember-addon",
    "lottie",
    "lottie-web",
    "ember",
    "animation",
    "animate",
    "vector",
    "graphics",
    "after effects"
  ],
  "homepage": "https://github.com/ClarkSource/ember-lottie#readme",
  "bugs": {
    "url": "https://github.com/ClarkSource/ember-lottie/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ClarkSource/ember-lottie.git"
  },
  "license": "MIT",
  "author": "Jan Buscht√∂ns <jan.buschtoens@clark.de>",
  "scripts": {
    "build": "ember build --environment=production",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*",
    "lint:hbs": "ember-template-lint .",
    "lint:js": "eslint --ext ts,js .",
    "lint:types": "tsc --noEmit",
    "start": "ember serve",
    "test": "npm-run-all lint:* test:*",
    "test:ember": "ember test",
    "test:ember-compatibility": "ember try:each",
    "prepack": "ember ts:precompile",
    "postpack": "ember ts:clean"
  },
  "dependencies": {
    "ember-cli-babel": "^7.21.0",
    "ember-cli-htmlbars": "^5.2.0",
    "ember-cli-typescript": "^4.0.0"
  },
  "devDependencies": {
    "@clark/ember-template-lint-config": "^0.2.2",
    "@clark/eslint-config-ember": "^1.28.1",
    "@clark/eslint-config-ember-typescript": "^1.28.2",
    "@clark/eslint-config-node": "^1.28.2",
    "@clark/prettier-config": "^1.28.2",
    "@ember/optional-features": "^2.0.0",
    "@glimmer/component": "^1.0.1",
    "@glimmer/tracking": "^1.0.2",
    "@types/ember": "^3.16.0",
    "@types/ember-qunit": "^3.4.9",
    "@types/ember-resolver": "^5.0.9",
    "@types/ember__test-helpers": "^1.7.2",
    "@types/qunit": "^2.9.1",
    "broccoli-asset-rev": "^3.0.0",
    "ember-auto-import": "^1.6.0",
    "ember-cli": "~3.21.2",
    "ember-cli-dependency-checker": "^3.2.0",
    "ember-cli-inject-live-reload": "^2.0.2",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-typescript-blueprints": "^3.0.0",
    "ember-cli-uglify": "^3.0.0",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-export-application-global": "^2.0.1",
    "ember-load-initializers": "^2.1.1",
    "ember-maybe-import-regenerator": "^0.1.6",
    "ember-qunit": "^4.6.0",
    "ember-resolver": "^8.0.0",
    "ember-source": "~3.21.1",
    "ember-source-channel-url": "^3.0.0",
    "ember-template-lint": "^2.12.1",
    "ember-try": "^1.4.0",
    "eslint": "^7.9.0",
    "loader.js": "^4.7.0",
    "npm-run-all": "^4.1.5",
    "qunit-dom": "^1.5.0",
    "release-it": "^14.0.3",
    "release-it-lerna-changelog": "^2.4.0",
    "typescript": "^4.0.2"
  },
  "engines": {
    "node": "10.* || >= 12"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "prettier": "@clark/prettier-config",
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config"
  },
  "release-it": {
    "plugins": {
      "release-it-lerna-changelog": {
        "infile": "CHANGELOG.md",
        "launchEditor": false
      }
    },
    "git": {
      "tagName": "v${version}"
    },
    "github": {
      "release": true,
      "tokenRef": "GITHUB_AUTH"
    }
  }
}
